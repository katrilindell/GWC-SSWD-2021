<h1>Member Data for
  <%= member.fullName %>
</h1>
<table class="table">
  <tr>
    <th>Name</th>
    <td>
      <%= member.fullName %>
    </td>
  </tr>
  <tr>
    <th>Email</th>
    <td>
      <%= member.email %>
    </td>
  </tr>
  <tr>
    <th>Zip Code</th>
    <td>
      <%= member.zipCode %>
    </td>
  </tr>
  <tr>
    <th>Password</th>
    <td class="hidden-text">
      <%= member.password %>
    </td>
  </tr>
</table>

<% } %>


<div class="center">
  <a class="button" href="/members">View all members</a>
</div>
